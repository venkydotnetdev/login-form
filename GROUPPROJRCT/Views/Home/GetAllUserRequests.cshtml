@model IEnumerable<GROUPPROJRCT.Models.RegistrationModel>

@{
    ViewBag.Title = "GetAllUserRequests";
}

<h2>GetAllUserRequests</h2>

<p>
    @Html.ActionLink("REGISTER", "Registration")
</p>
<table class="table">
    <tr>
        <th>
            FirstName
        </th>
        <th>
            LastName
        </th>
        <th>
            Email
        </th>
        <th>
            PhoneNumber
        </th>
        <th>
            Role
        </th>
        <th>
            Deparmemnt
        </th>
        <th>
            Password
        </th>
        <th>
            Gender
        </th>
        <th>
            Cource
        </th>
        <th>
            Job
        </th>
        <th>
            Designation
        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Role)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deparmemnt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cource)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Job)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Designation)
            </td>
        </tr>
    }

</table>




